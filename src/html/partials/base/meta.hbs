<!-- {{! See variable definitions in template-data/meta.json }} -->
<title>{{meta.title}}</title>
<meta charset='utf-8'>
<meta name='viewport' content='width=device-width'>
<meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'>
<meta name='description' content='{{meta.description}}' />
<meta name='news_keywords' content='{{meta.keywords}}' />
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

<meta property='og:title' content='{{meta.title}}' />
<meta property='og:site_name' content='New American Economy' />
<meta property='og:url' content='{{meta.url}}' />
<meta property='og:image' content='{{meta.imgFacebook}}' />
<meta property='og:image:type' content='image/png' />
<meta property='og:image:width' content='1200' />
<meta property='og:image:height' content='675' />
<meta property='og:description' content='{{meta.description}}' />
<meta property='og:type' content='article' />
<meta property='og:locale' content='en_US' />

<meta name='twitter:card' content='summary_large_image' />
<meta name='twitter:site' content='http://data.newamericaneconomy.org' />
<meta name='twitter:creator' content='@newamericanecon' />
<meta property='twitter:title' content='{{meta.title}}' />
<meta property='twitter:description' content='via @newamericanecon: Data Briefs https://data.newamericaneconomy.org' />
<meta property='twitter:image:src' content='http://data.newamericaneconomy.org/assets/<ADD IMAGE FILE NAME>' />
<meta name="twitter:image:alt" content="{{meta.title}}">

<meta name="msapplication-TileImage" content='{{meta.msapplication-TileImage}}'/>
<meta name="msapplication-TileColor" content='{{meta.msapplication-TileColor}}' />